OBJS = router.o client.o msg.o 
CFLAGS = -std=c++17 -g
DBUILD = ../build/
INC = -I ../include/


all: containers events map world this


containers:
	$(MAKE) -C ./containers
	
events:
	$(MAKE) -C ./events
	
map:
	$(MAKE) -C ./map
	
world:
	$(MAKE) -C ./world
	
	
this: $(OBJS)


%.o: %.cpp
	$(CXX) $(CFLAGS) $(INC) -o $(DBUILD)$@ -c $<
